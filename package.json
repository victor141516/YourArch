{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "run-compiled": "node --experimental-specifier-resolution=node",
    "lint": "eslint src/**",
    "lint:tsc": "tsc --noEmit",
    "prisma:generate": "yes | npx prisma generate",
    "prisma:infer": "yes | npx prisma db pull",
    "prisma:migrate": "yes | npx prisma db push"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  },
  "eslintIgnore": [
    "/**/*.js"
  ],
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "express": "^4.17.2",
    "html-entities": "^2.3.2",
    "jsdom": "^17.0.0",
    "node-fetch": "^3.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "striptags": "^3.2.0",
    "youtube-captions-scraper": "github:victor141516/youtube-captions-scraper"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.13",
    "@types/node-fetch": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
